@{
    ViewBag.Title = "About Us";
}
<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.0/css/all.min.css">
</head>

<body>
	<div class="hero">
		<div class="container">
			<div class="row justify-content-between">
				<div class="col-lg-5">
					<div class="intro-excerpt">
						<h1>About Us</h1>
						<p><a href="" class="btn btn-secondary me-2">Shop Now</a></p>
					</div>
				</div>
				<div class="col-lg-7">
					<div class="hero-img-wrap">
						<img src="~/2514-removebg-preview.png" />
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class="untree_co-section">
		<div class="container">
			<div class="row mb-5">
				<div class="col-lg-12 text-center">
					<h2 class="section-title">Our Team</h2><br/>
				</div>
			</div>
			<div class="row text-center">
				<div class="col-12 col-md-4 mb-5 mb-md-0">
					<h3 class="section-title"><u><span>Umair Waseem</span></u></h3>
					<span class="d-block position mb-4">Developer</span>
				</div>
				<div class="col-12 col-md-4 mb-5 mb-md-0">
					<h3 class="section-title"><u><span>Zain Imran</span></u></h3>
					<span class="d-block position mb-4">Developer</span>
				</div>
				<div class="col-12 col-md-4 mb-5 mb-md-0">
					<h3 class="section-title"><u><span>Adeel Amanat</span></u></h3>
					<span class="d-block position mb-4">Developer</span>
				</div>
			</div>
		</div>
	</div>

	<div class="why-choose-section">
		<div class="container">
			<div>
					<h2 class="section-title">Why Choose Us</h2>

					<div class="row my-5">
						<div class="col-md-6">
							<div class="feature">
								<div class="icon">
									<img src="/truck.svg" alt="Image" class="img-fluid">
								</div>
								<h3>Fast & Free Shipping</h3>
								<p>We assure that your order will be shipped within 15 days depending upon your address.</p>
							</div>
						</div>

						<div class="col-md-6">
							<div class="feature">
								<div class="icon">
									<img src="/bag.svg" alt="Image" class="img-fluid">
								</div>
								<h3>Easy to Shop</h3>
								<p>Shopping has become easier for you. Email us the list of products you want & we'll respond to it.</p>
							</div>
						</div>

						<div class="col-md-6 mt-3 mt-md-0">
							<div class="feature">
								<div class="icon">
									<img src="/support.svg" alt="Image" class="img-fluid">
								</div>
								<h3>24/7 Support</h3>
								<p>If you have any queries, we are available to assist you.</p>
							</div>
						</div>

						<div class="col-md-6 mt-3 mt-md-0">
							<div class="feature">
								<div class="icon">
									<img src="/return.svg" alt="Image" class="img-fluid">
								</div>
								<h3>Hassle Free Returns</h3>
								<p>If the product you received has any defects or issues then return it to us.</p>
							</div>
						</div>
					</div>
			</div>
		</div>
	</div>

	<footer class="footer-section">
		<div class="container">
			<div class="row g-5 mb-5">
				<div class="col-lg-4">
					<ul class="list-unstyled custom-social">
						<li><a href="https://www.facebook.com/gold.figure2008?mibextid=kFxxJD" target="_blank"><span class="fa fa-brands fa-facebook"></span></a></li>
						<li><a href="https://www.instagram.com/gold.figure/" target="_blank"><span class="fa fa-brands fa-instagram"></span></a></li>
						<li><a href="https://pk.linkedin.com/in/gold-figure-745934b5" target="_blank" ><span class="fa fa-brands fa-linkedin"></span></a></li>
					</ul>
				</div>
			</div>
		</div>

		<div class="container border-top copyright">
			<div class="row pt-4">
				<div class="col-lg-6">
					<p class="mb-2 text-center text-lg-start">
						Copyright ©
						2024. All Rights Reserved For Gold-Figure — Designed By<br/> UMAIR WASEEM, ZAIN IMRAN, ADEEL AMANAT
					</p>
				</div>

				<div class="col-lg-6 text-center text-lg-end">
					<ul class="list-unstyled d-inline-flex ms-auto">
						<li class="me-4"><a href="#">Terms &amp; Conditions</a></li>
						<li><a href="#">Privacy Policy</a></li>
					</ul>
				</div>

			</div>
		</div>

		</div>
	</footer>
	<script src="js/bootstrap.bundle.min.js"></script>
	<script src="js/tiny-slider.js"></script>
	<script src="js/custom.js"></script>
</body>

</html>

